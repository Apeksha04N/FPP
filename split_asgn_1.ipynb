   "source": [
    "def cstolot(list):\n",
    "    result = []\n",
    "    for i in list.split(';'):\n",
    "        result.append(tuple(i.split('=')))\n",
    "    return result\n",
    "list = \"a=b;c=d;e=f;g=h\"\n",
    "cstolot(list)"
   ]
